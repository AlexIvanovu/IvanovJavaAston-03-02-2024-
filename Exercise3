public class Exercise3 {
    public static void main(String[] args) {
        int [] mas = new int[] {-3,0, 11, 12, 77, 33, -66};
        for (int i=0; i< mas.length; i++) {
            if (mas[i]%3==0) {
                System.out.println(mas[i]);
            }
        }


        for (int i=0; i< mas.length; i++) {
            if (mas[i]%3==0) {
                System.out.println(mas[i]);
            }
        }
    }
}
